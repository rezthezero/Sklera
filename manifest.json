{
  "name": "Sklera",
  "description": "We care about eye care!",
  "version": "1.0",
  "manifest_version": 3,
  "action": 
  {
    "default_title": "Click to view a popup",
    "default_popup": "popupv2.html"
  },
  "background" : 
  {
    "service_worker" : "background.js"
  },
  "permissions" : ["storage","scripting", "activeTab","tabs"],
  "host_permissions":["http://*/*", "http://*/", "https://*/"]

}